{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.15.0",
    "generators": {
      "java": {
        "output": "./java-service",
        "inputSpec": "./mocks/swagger.json",
        "generatorName": "java",
        "additionalProperties": {
          "dateLibrary": "java8-localdatetime",
          "library": "retrofit2",
          "serializationLibrary": "jackson",
          "enumPropertyNaming": "legacy"
        },
        "globalProperty": {
          "models": "",
          "supportingFiles": "JSON.java,models/AbstractOpenApiSchema.java",
          "modelTests": "false",
          "modelDocs": "false"
        }
      }
    }
  }
}
